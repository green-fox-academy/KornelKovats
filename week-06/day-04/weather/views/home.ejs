            <div><%= cast[i].location%></div>
         </div>
         <div  class="temperature">
                <div><%= cast[i].weather[0].temp%> Â°C</div>
         </div>
         <div class="picture">
            <img src="static/<%=cast[i].weather[0].icon%>.png" alt="">
         </div>

    </div>
    <%}%>
</div>
</body>
</html>